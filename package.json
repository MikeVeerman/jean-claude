{
  "name": "jean-claude",
  "version": "1.3.0",
  "description": "Sync Claude Code configuration across machines using Git",
  "author": "Mike Veerman",
  "repository": {
    "type": "git",
    "url": "https://github.com/MikeVeerman/jean-claude"
  },
  "homepage": "https://github.com/MikeVeerman/jean-claude#readme",
  "type": "module",
  "main": "./dist/index.js",
  "bin": {
    "jean-claude": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "dev": "tsx src/index.ts",
    "test": "npm run test:unit && npm run test:integration",
    "test:unit": "vitest run",
    "test:unit:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:integration": "./test-integration.sh",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "claude",
    "claude-code",
    "dotfiles",
    "sync",
    "cli",
    "configuration"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=18"
  },
  "files": [
    "dist",
    "templates"
  ],
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^12.0.0",
    "fs-extra": "^11.2.0",
    "inquirer": "^9.2.0",
    "simple-git": "^3.22.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^9.0.7",
    "@types/node": "^20.11.0",
    "@vitest/coverage-v8": "^1.6.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0",
    "vitest": "^1.2.0"
  }
}
